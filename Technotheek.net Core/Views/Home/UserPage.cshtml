
@{
    ViewData["Title"] = "User";
}

<h1>User</h1>

<div>
    <div class="mt-2">
        <h4>Full name</h4>
        <span>@ViewBag.FirstName @ViewBag.LastName</span>
    </div>
    <div class="mt-2">
        <h4>Username</h4>
        <span>@ViewBag.Email</span>
    </div>
    <div class="mt-2">
        <h4>Adress</h4>
        <span>@ViewBag.Street @ViewBag.StreetNmr</span>
    </div>
</div>